<!-- #content -->
<div id="footer">
    <div class="container">
        <p><% if(theme.creative_commons) { %>
            <span class="cc-license">
                <a href="https://creativecommons.org/licenses/<%- theme.creative_commons %>/4.0" target="_blank" title="Creative Commons license <%- theme.creative_commons %>">
                <% theme.creative_commons.split('-').forEach(function(icon){ %>
                    <i class="cc cc-<%- icon %>"></i>
                <% }); %>
                </a>
            </span>
            <%- new Date().getFullYear() %> <%= config.title %>.
        <% } else { %>
            &copy; <%- new Date().getFullYear() %> <%= config.title %> All Rights Reserved. 
        <% } %></p>
        <p>Themed by <a href="http://mufeng.me/">Kunkka</a> | Powered by <a href="https://hexo.io/">Hexo</a>.</p>
        <%- partial('_partial/sns') %>
    </div>
</div>
