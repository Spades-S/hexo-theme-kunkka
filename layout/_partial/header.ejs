<head>
    <title>
    <% if(page.archive) { %>
        <%= page.year || page.month || __('archive') %> - 
    <% } else if (page.title || page.category || page.tag) { %>
        <%= page.title || page.category || page.tag %> - 
    <% } %>
    <%= config.title || (config.author + "\'s Blog") %>
    </title>
    
    <meta name="keywords" content="<%= page.keywords || '' %>">
    <meta name="description" content="<%= page.description || '' %>">
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
    <meta name="renderer" content="webkit|ie-comp|ie-stand">
    <meta name="viewport" content="width=device-width,initial-scale=1,maximum-scale=1,minimum-scale=1,user-scalable=no">
    
    <% var feed = ''; %>
    <% if(config.feed) { %>
        <% feed = config.feed.type; %>
        <% if(feed == 'rss2') { %>
            <% feed = 'rss'; %>
        <% } %>
    <link rel="alternate" type="<%- feed %>" hrel="<%- url_for(config.feed.path) %>">
    <% } %>
    
    <% if(theme.favicon) { %>
    <link rel="Shortcut Icon" href="<%- theme.favicon %>" type="image/x-icon"/>
    <% } %>
    
    <%- css('css/style.css') %>
    <link href="https://cdn.bootcss.com/font-awesome/4.6.3/css/font-awesome.min.css" rel="stylesheet" type="text/css">
    
    <%- js('js/jquery.min.js') %>
    <%- js('js/base.min.js') %>
    <%- js('js/all.min.js') %>
</head>
