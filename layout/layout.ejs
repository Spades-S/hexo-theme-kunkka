<!DOCTYPE html>
<html lang="<%= config.lang %>">

<!-- Head tag -->
<%- partial('_partial/head',{cache: !config.relative_link}) %>

<body class="<%- trim(partial('_scripts/body_class')) %>">
    <%- partial('_partial/nav') %>
    <div id="content">
        <div class="container clearfix">
            <!-- Main Content -->
            <%- body %>
            <%- partial('_partial/sidebar',{cache: !config.relative_link}) %>
        </div>
    </div>
    <!-- Footer -->
    <%- partial('_partial/footer',{cache: !config.relative_link}) %>
    <%- partial('_partial/after_footer',{cache: !config.relative_link}) %>
</body>

</html>
