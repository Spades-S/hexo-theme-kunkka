---
layout: layout
---
<div id="primary">
    <div>
        <div>
            <h2 class="post-title">
                <a href="<%- url_for(page.path) %>" title="<%- page.title %>"><%- page.title %></a>
            </h2>
        </div>
        <div class="post-body clearfix">
            <div class="post-content">
                <%- page.content || body %>
            </div>
        </div>
    </div>
    <%- partial('_partial/comments') %>
</div>
<!-- #primary -->
