---
layout: layout
---
<div id="primary">
    <div>
        <div>
            <h1 class="post-title">
                <a href="<%- url_for(page.path) %>" title="<%= page.title %>"><%= page.title %></a>
            </h1>
        </div>
        <div class="post-body clearfix">
            <div class="post-content">
                <%- page.content || body %>
            </div>
        </div>
    </div>
</div><!-- #primary -->
<%- partial('_partial/comments') %>
